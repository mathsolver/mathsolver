{
    "name": "mathsolver/mathsolver",
    "description": "PHP package to simplify mathematical expressions and solve equations.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Jeroen van Rensen",
            "role": "Developer",
            "homepage": "https://www.jeroenvanrensen.nl"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "MathSolver\\": "src/"
        }
    },
    "require-dev": {
        "pestphp/pest": "dev-master",
        "phpstan/phpstan": "^1.4"
    },
    "scripts": {
        "test": "./vendor/bin/pest",
        "analyse": "vendor/bin/phpstan analyse src tests --level=5"
    },
    "require": {
        "illuminate/support": "^8.80",
        "illuminate/collections": "^8.80"
    }
}
